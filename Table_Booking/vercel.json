{
    "version": 2,
    "builds": [
      {
        "src": "js/server.js",
        "use": "@vercel/node"   
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.html"    
      },
      {
        "src": "/send-email",   
        "dest": "/js/server.js"
      }
    ]
  }
  